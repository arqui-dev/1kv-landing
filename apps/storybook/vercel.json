{
  "buildCommand": "npm run build",
  "outputDirectory": "storybook-static",
  "framework": "vite",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://*.vercel.app https://1kvideos.com https://www.1kvideos.com https://*.1kvideos.com http://localhost:5173;"
        }
      ]
    }
  ]
}
